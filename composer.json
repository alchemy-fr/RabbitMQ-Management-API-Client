{
    "name": "vidaxl/rabbitmq-management-client",
    "type": "library",
    "description": "RabbitMQ Management Plugin API Client",
    "keywords": ["api", "rabbitmq", "management"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Neutron",
            "email": "imprec@gmail.com"
        }
    ],
    "require": {
        "doctrine/common": ">=2.0",
        "guzzlehttp/guzzle": "^5.3|^6.0"
    },
    "require-dev": {
        "videlalvaro/php-amqplib": "dev-master",
        "sami/sami": "~1.0",
        "phpunit/phpunit": "~3.7",
        "react/promise": "~1.0",
        "react/react": "~0.2.0",
        "react/curry": "~1.0"
    },
    "autoload": {
        "psr-0": {
            "RabbitMQ": "src"
        }
    },
    "suggest": {
        "react/react": "Version 0.2 to use the async client"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
