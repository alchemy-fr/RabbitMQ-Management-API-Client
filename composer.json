{
    "name": "alchemy/rabbitmq-management-client",
    "type": "library",
    "description": "RabbitMQ Management Plugin API Client",
    "keywords": ["api", "rabbitmq", "management"],
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin/"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Neutron",
            "email": "imprec@gmail.com"
        }
    ],
    "require": {
        "guzzle/guzzle": "2.8.*",
        "react/promise": "1.0.*",
        "react/react": "0.2.*",
        "react/curry": "1.0.*",
        "doctrine/common": ">=2.0"
    },
    "require-dev": {
        "videlalvaro/php-amqplib": "dev-master",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "RabbitMQ": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
